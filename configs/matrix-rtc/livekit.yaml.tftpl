port: 7880
log_level: info

room:
  auto_create: false

prometheus:
  port: 9091

rtc:
  tcp_port: 7881
  port_range_start: 59000
  port_range_end: 60000
  use_external_ip: true

redis:
  address: localhost:6379

turn:
  enabled: true
  domain: turn.${base_domain}
  udp_port: 3478
  tls_port: 5349
  external_tls: true

key_file: /run/secrets/matrix-rtc-livekit-keys
